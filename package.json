{
  "name": "appium-webdriverio-jasmine-e2e-test",
  "version": "0.1.0",
  "description": "A working solution using Appium with JavaScript client WebriverIO and Jasmine for E2E mobile testing",
  "scripts": {
    "test": "./node_modules/.bin/wdio ./configs/wdio.conf.js",
    "local": "./node_modules/.bin/wdio ./configs/wdio.conf.local.js",
    "sauce": "./node_modules/.bin/wdio ./configs/wdio.conf.saucelabs.js",
    "report": "./node_modules/.bin/allure generate --clean reports/allure-results --output reports/allure-report"
  },
  "keywords": [
    "mobile",
    "appium",
    "e2e",
    "jasmine",
    "webdriverio",
    "selenium",
    "behaviour driven development"
  ],
  "author": "Quintus Chen <quintus.guo.chen@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "appium": "^1.7.2",
    "webdriverio": "^4.12.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "wdio-allure-reporter": "^0.6.0",
    "wdio-appium-service": "^0.2.3",
    "wdio-jasmine-framework": "^0.3.2",
    "wdio-spec-reporter": "^0.1.4"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
